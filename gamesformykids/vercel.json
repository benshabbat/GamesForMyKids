{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev", 
  "installCommand": "npm install",
  "framework": "nextjs",
  "functions": {
    "app/**": {
      "runtime": "@vercel/next@14.2.13"
    }
  },
  "routes": [
    {
      "src": "/_next/static/(.*)",
      "dest": "/_next/static/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
